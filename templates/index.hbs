<html>

<head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="./css/telegraph.css">
    {{!-- font awesome --}}
    <script src="https://use.fontawesome.com/8315d2853f.js"></script>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-107075173-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-107075173-1');
    </script>
    <title>Shane's Rusty Blog</title>
</head>

<body>
    <div class="page">
        <h1>Shane's Rusty Blog</h1>
        <br>
        <p>
            {{> contact}}
        </p>
        <br>
        <ul>
            {{#each . as |post|}}
            <li>
                <a href="./{{post.permalink}}">{{post.title}}</a>
            </li>
            {{/each}}
        </ul>
    </div>
</body>

</html>